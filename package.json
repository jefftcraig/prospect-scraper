{
  "name": "prospect-scraper",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^14.9.0"
  },
  "description": "Scrape prospects from multiple development leagues",
  "main": "",
  "jest": {
    "verbose": true,
    "modulePathIgnorePatterns": [
      "__fixtures__"
    ]
  },
  "scripts": {
    "test": "jest --watch",
    "test:no-watch": "jest",
    "lint": "eslint --ignore-path ../.gitignore .",
    "prettier": "prettier --ignore-path ../.gitignore \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm run check-format && npm run lint && npm run test",
    "scrape:games": "node scripts/run-games-scraper",
    "scrape:seasons": "node scripts/run-season-scraper",
    "twitter:run": "node twitter/bot.js"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.2.0",
    "knex": "^0.21.12",
    "pg": "^8.2.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "twit": "^2.2.11"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "event-stream": "^4.0.1",
    "jest": "^26.0.1",
    "nock": "^12.0.3",
    "prettier": "^2.0.5"
  },
  "author": "Jeff Craig",
  "license": "ISC"
}
